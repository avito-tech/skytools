-- test triggers
create table tgfk (
  id int4 primary key,
  data text
);
create table tgtest (
  id int4 primary key,
  fk int4 references tgfk,
  data text
);
create or replace function notg() returns trigger as $$
begin
    return null;
end;
$$ language plpgsql;
create trigger tg_nop after insert on tgtest for each row execute procedure notg();
